.player-mode-element {
    position: relative;
    flex-direction: row;
    align-items: flex-start;
    justify-content: center;
    flex: 1;
}

.symbol-label {
    -unity-font-style: bold;
    font-size: 32px;
    color: var(--black);
}

.mode-label {
    -unity-font-style: italic;
    font-size: 24px;
    color: var(--dark-green);
} 